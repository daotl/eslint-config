{
  "name": "@daotl/eslint-config",
  "type": "module",
  "version": "2.0.0-alpha.10",
  "packageManager": "pnpm@8.9.0",
  "description": "ESLint configs for DAOT projects",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/web-configs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/web-configs.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/web-configs/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "browser",
    "frontend"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm",
    "dev": "tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "biome format . && biome check .",
    "lint:fix": "biome format --write . && biome check --apply .",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix ."
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">=8.51.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.26",
    "@phaphoso/eslint-plugin-dprint": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "globals": "^13.23.0",
    "local-pkg": "^0.5.0"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "typescript": "^5.2.2"
  }
}
