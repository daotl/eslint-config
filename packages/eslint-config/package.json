{
  "name": "@daotl/eslint-config",
  "version": "1.0.0",
  "packageManager": "pnpm@8.7.0",
  "description": "ESLint configs for DAOT projects",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/web-configs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/web-configs.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/web-configs/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "dprint",
    "typescript",
    "browser",
    "frontend"
  ],
  "main": "index.js",
  "scripts": {
    "test": "eslint . --config=typescript.js",
    "lint": "biome format . && biome check .",
    "lint:fix": "biome format --write . && biome check --apply ."
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "dprint": ">=0.41.0",
    "eslint": ">=8.49.0"
  },
  "dependencies": {
    "@antfu/eslint-config-basic": "^0.42.0",
    "@antfu/eslint-config-ts": "^0.42.0",
    "@daotl/dprint-config": "workspace:*",
    "@phaphoso/eslint-plugin-dprint": "^0.5.2",
    "@typescript-eslint/eslint-plugin": ">=6.7.0",
    "@typescript-eslint/parser": ">=6.7.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-import-resolver-webpack": "^0.13.7",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-tsdoc": "^0.2.17"
  },
  "devDependencies": {
    "dprint": "^0.41.0",
    "eslint": "^8.49.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2"
  }
}