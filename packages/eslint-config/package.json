{
  "name": "@daotl/eslint-config",
  "version": "1.0.0-alpha.6",
  "packageManager": "pnpm@7.27.0",
  "description": "ESLint configs for DAOT projects",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/web-configs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/web-configs.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/web-configs/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "dprint",
    "typescript",
    "browser",
    "frontend"
  ],
  "main": "index.js",
  "scripts": {
    "test": "eslint . --config=typescript.js",
    "lint": "rome format . && rome check .",
    "lint:fix": "rome format --write . && rome check --apply ."
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "dprint": ">=0.34.5",
    "eslint": ">=8.34.0"
  },
  "dependencies": {
    "@antfu/eslint-config-basic": "^0.35.2",
    "@antfu/eslint-config-ts": "^0.35.2",
    "@daotl/dprint-config": "workspace:*",
    "@phaphoso/eslint-plugin-dprint": "^0.5.2",
    "@typescript-eslint/eslint-plugin": ">=5.52.0",
    "@typescript-eslint/parser": ">=5.52.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-tsdoc": "^0.2.17"
  },
  "devDependencies": {
    "dprint": "^0.34.5",
    "eslint": "^8.34.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  }
}
